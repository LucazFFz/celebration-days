{
    "manifest_version": 2,
    "name": "Celebration Days",
    "version": "0.1.0",
    "content_scripts": [
        {
            "matches": ["<all_urls>"],
            "js": ["scripts/content.js"]
        }
    ],
    "browser_action": {
        "default_popup": "popup.html",
        "default_title": "Celebration Days"
    },
    "icons": {
        "16": "./images/logo-16.png",
        "32": "./images/logo-32.png",
        "128": "./images/logo-128.png"
    }
    
}