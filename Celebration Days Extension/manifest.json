{
    "manifest_version": 2,
    "name": "Celebration Days",
    "version": "0.1.0",
    "content_scripts": [
        {
            "matches": ["<all_urls>"],
            "js": ["scripts/content.js"]
        }
    ],
    "browser_action": {
        "default_popup": "popup.html",
        "default_title": "Celebration Days"
    }
}